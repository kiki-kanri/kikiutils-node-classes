{
  "name": "@kikiutils/classes",
  "version": "3.0.0",
  "description": "A utility library providing various classes for advanced operations and manipulations.",
  "license": "MIT",
  "author": "kiki-kanri",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiki-kanri/kikiutils-node-classes.git"
  },
  "main": "./index.cjs",
  "module": "./index.mjs",
  "scripts": {
    "build": "ts-project-builder ./src/**/*.ts --clean",
    "bumplog": "changelogen --bump"
  },
  "types": "./index.d.ts",
  "dependencies": {
    "@kikiutils/fs-extra": "^2.3.4",
    "consola": "^3.2.3",
    "decimal.js": "^10.4.3"
  },
  "devDependencies": {
    "@kikiutils/prettierrc": "^1.3.2",
    "@kikiutils/tsconfigs": "^3.0.1",
    "@types/node": "^22.5.5",
    "@types/ssh2": "^1.15.1",
    "changelogen": "^0.5.7",
    "node-ssh": "^13.2.0",
    "ts-project-builder": "^3.0.1",
    "tslib": "^2.7.0"
  },
  "keywords": [
    "arithmetic",
    "decimal",
    "file system",
    "fs",
    "fs-extra",
    "math",
    "number",
    "path",
    "precision",
    "utility",
    "utils"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": {
    "./*": {
      "import": "./*.mjs",
      "require": "./*.cjs",
      "types": "./*.d.ts"
    }
  },
  "prettier": "@kikiutils/prettierrc"
}

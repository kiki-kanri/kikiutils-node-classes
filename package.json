{
  "name": "@kikiutils/classes",
  "version": "2.1.1",
  "description": "A utility library providing various classes for advanced operations and manipulations.",
  "license": "MIT",
  "author": "kiki-kanri",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiki-kanri/kikiutils-node-classes.git"
  },
  "main": "./index.cjs",
  "module": "./index.mjs",
  "scripts": {
    "build": "rollup -c --configPlugin typescript=\"{ tsconfig: './tsconfig.rollup.json' }\"",
    "bumplog": "changelogen --bump"
  },
  "types": "./index.d.ts",
  "dependencies": {
    "@kikiutils/fs-extra": "^2.3.3",
    "decimal.js": "^10.4.3"
  },
  "devDependencies": {
    "@kikiutils/prettierrc": "^1.3.2",
    "@kikiutils/tsconfigs": "^3.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.14.11",
    "changelogen": "^0.5.5",
    "rollup": "^4.19.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-node-externals": "^7.1.2"
  },
  "keywords": [
    "arithmetic",
    "decimal",
    "file system",
    "fs",
    "fs-extra",
    "math",
    "number",
    "path",
    "precision",
    "utility",
    "utils"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    },
    "./*": {
      "import": "./*.mjs",
      "require": "./*.cjs",
      "types": "./*.d.ts"
    },
    "./_index": {
      "import": null,
      "require": null,
      "types": null
    }
  },
  "prettier": "@kikiutils/prettierrc"
}
